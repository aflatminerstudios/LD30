            <kind>1</kind>
            <string>///Set Options using gamepad

hVal = gamepad_axis_value(0, gp_axislh);
vVal = gamepad_axis_value(0, gp_axislv);

if (useGamePad) {

    if (vVal &gt; 0.5 &amp;&amp; vVal &gt; hVal) {
        if (selectedOption != optCredits) {
            choiceBox.y += 43;
            selectedOption += 1;
        }
        useGamePad = false;    
    } else if (vVal &lt; -0.5 &amp;&amp; vVal &lt; hVal) {
        if (selectedOption != optStart) {
            choiceBox.y -= 43;
            selectedOption -= 1;
        }
        useGamePad = false;    
    }

} else {
    if (abs(hVal) &lt; 0.25 &amp;&amp; abs(vVal) &lt; 0.25){ 
        useGamePad = true;
    }
}

///Check for gamepad button

if (scrCheckController(0) == true)
{
   alarm[0] = 1;
}
</string>
